<?xml version="1.0" encoding="UTF-8"?>
<jaxb:bindings xmlns:jaxb="http://java.sun.com/xml/ns/jaxb"
               xmlns:xs="http://www.w3.org/2001/XMLSchema"
               version="1.0"
>

    <jaxb:globalBindings generateElementProperty="false">
        <jaxb:javaType name="java.time.LocalDateTime" xmlType="xs:dateTime"
                       parseMethod="se.sundsvall.businessinformation.util.DateAdapter.parseDateTime"
                       printMethod="se.sundsvall.businessinformation.util.DateAdapter.printLocalDateTime"/>
        <jaxb:javaType name="java.time.LocalDate" xmlType="xs:date"
                       parseMethod="se.sundsvall.businessinformation.util.DateAdapter.parseDate"
                       printMethod="se.sundsvall.businessinformation.util.DateAdapter.printLocalDate"/>
    </jaxb:globalBindings>
</jaxb:bindings>
